<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <ScrollView
        android:layout_width="400dp"
        android:layout_height="200dp"
        android:id="@+id/scrollView2"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <LinearLayout android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="">

            <TextView
                android:text="Complemento Práctica 1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="24dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="31dp"
                android:id="@+id/textView" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/textView8"
                android:layout_alignLeft="@+id/textView8"
                android:layout_alignStart="@+id/textView8"
                android:layout_marginTop="24dp">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="Función Química"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_column="1"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:text="Grupo Funcional"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_column="2"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_column="3"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="Nomenclatura" />
                    /></TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_column="1"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="Halogenuro de Alquilo" />

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_column="2"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="-X (X= halógeno)" />

                    <TextView
                        android:text="Nombre del \nhalógeno + \nnombre del \nhidrocarburo."
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_column="3"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>
                    /></TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >
                    <TextView
                        android:text="Alcohol"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_column="1"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:text="R-OH"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_column="2"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_column="3"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="Nombre del \nhidrocarburo \nterminado en ol." />
                    /></TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:text="Éter"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="1"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="2"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="R-O-R’" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="3"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="Nombre del radical \nmenos complejo \nterminado en oxi + \nnombre del \nhidrocarburo \n(radical más \ncomplejo)." />
                    /></TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:text="Cetona"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="1"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="2"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="R-CO-R’" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="3"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="Nombre del \nhidrocarburo \nterminado en ona." />
                    /></TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:text="Aldehído"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="1"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="2"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="R-CHO" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="3"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="Nombre del \nhidrocarburo \nterminado en al." />
                    /></TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:text="Ácido carboxílico"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="1"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="2"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="R-COOH" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="3"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="Ácido + nombre del \nhidrocarburo \nterminado en oico."/>
                    /></TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:text="Sal orgánica"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="1"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="2"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="R-COOM" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="3"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="Nombre del \nhidrocarburo \nterminado en ato \n+ nombre del metal."/>
                    /></TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:text="Éster"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="1"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="2"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="R-COO-R’" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="3"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="Nombre del \nhidrocarburo \nterminado en ato + \nnombre del radical \nalquilo."/>
                    /></TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:text="Amina"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="1"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="2"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="R-NH2" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="3"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="Amino + nombre del \nhidrocarburo."/>
                    /></TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:text="Amida"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="1"
                        android:padding="3dp"
                        android:paddingRight="3dp"/>

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="2"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="R-CONH2" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_column="3"
                        android:padding="3dp"
                        android:paddingRight="3dp"
                        android:text="Nombre del \nhidrocarburo \nterminado en amida."/>
                    /></TableRow>

            </TableLayout>

        </LinearLayout>
    </ScrollView>

    <VideoView
        android:id="@+id/surface_view"
        android:layout_width="320px"
        android:layout_height="240px"/>
</RelativeLayout>